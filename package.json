{
  "name": "diagram-editor-widget",
  "version": "1.0.0",
  "description": "A modular, plugin-based visual graph editor for TypeScript applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:bundle",
    "build:tsc": "tsc",
    "build:bundle": "rollup -c",
    "build:watch": "tsc --watch",
    "dev": "concurrently \"npm run build:watch\" \"python3 -m http.server 3000\"",
    "serve": "python3 -m http.server 3000",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "diagram",
    "graph",
    "editor",
    "visual",
    "workflow",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/d3": "^7.4.2",
    "@types/d3-array": "^3.0.5",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-zoom": "^3.0.3",
    "@types/geojson": "^7946.0.10",
    "concurrently": "^8.2.2",
    "rollup": "^4.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}